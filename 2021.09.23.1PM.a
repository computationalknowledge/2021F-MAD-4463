	int Result = 0;
	
	public int AddNumbers(int startNumber, int endNumber, int stepNumber) {
		
		for ( int counter = startNumber; counter <= endNumber ; counter += stepNumber) {
			Result += counter;
		}
		
		return Result;
	}
}
